@model BugTracker2.Models.Tickets
@{
    ViewBag.Title = "TicketNotify";
}

<div class="row">
    <div class="col-lg-6 col-lg-offset-3">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4>Ticket Notifications</h4>
            </div>
            <div class="panel-body">
                <div class="col-md-12 subnav">
        <h4>TicketNotify: @Html.ActionLink(Model.Title, "Details", new { id = Model.Id })</h4>
                    <h5>Notification Sent To: </h5>
                    @{ var ticketNotify = Model.TicketNotify; }
                       @foreach (var notify in ticketNotify.Reverse())
                       {
                        <h5>@notify.TicketId</h5>
                        <h5>@notify.NotifyUser.FullName</h5>
                        <h5>@notify.NotifyUser.Email</h5>
                        }
 </div>   <!-- /.box-body -->
       </div>  <!-- /.box -->
             </div>   <!-- /.col -->
                   </div>
                          </div>




