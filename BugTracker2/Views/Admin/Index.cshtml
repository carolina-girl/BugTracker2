@model IEnumerable<BugTracker2.Models.ApplicationUser>

<h2>User List</h2>

  

@foreach (var item in Model)
{

    <p>
        @Html.DisplayFor(modelItem => item.Email)
        @Html.ActionLink("Assign Roles", "EditUser", "Admin", new { id = item.Id }, null) |
        @Html.ActionLink("Assign Projects", "ProjectUser", "Admin", new { id = item.Id }, null)
    </p>
}

