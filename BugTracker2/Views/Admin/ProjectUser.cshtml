@model BugTracker2.Models.AdminProjectUserAssignViewModel
@{
    ViewBag.Title = "ProjectUser";
}



<h2>Project List</h2>

@using (Html.BeginForm())
{
    @Html.HiddenFor(model => model.User.Id)

    @Html.AntiForgeryToken()
    @Html.ListBox("AddSelectedProjectUsers", Model.Users)
    @Html.ListBox("RemoveSelectedProjectUsers", Model.Users)

    <input type="submit" value="Save" class="btn btn-default" />
}

